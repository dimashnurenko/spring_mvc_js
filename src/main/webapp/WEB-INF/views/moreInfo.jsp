<div id="main" class="moreInfoGeneral">
    <div id="backBtn" class="backBtn">
        <img class="backBtnImage" src="/resources/images/backBtn.svg"/>
        <label class="backLabel">Back to list</label>
    </div>

    <div id="moreInfo" class="moreInfo">
        <div class="address">
            <label class="addressLabel">Address:</label>

            <div class="addressForm">
                <label class="formLabelAddress">Country:</label>
                <input id="country" type="text" class="textFieldAddress">
                <label class="formLabelAddress">City:</label>
                <input id="city" type="text" class="textFieldAddress">
                <label class="formLabelAddress">Street:</label>
                <input id="street" type="text" class="textFieldAddress">
                <label class="formLabelAddress">House:</label>
                <input id="house" type="text" class="textFieldAddress">
                <label class="formLabelAddress">Flat:</label>
                <input id="flat" type="text" class="textFieldAddress">
            </div>

            <div class="buttonsPanelAddress">
                <div id="saveAddress" class="buttonAddress">
                    <label>Save</label>
                </div>
                <div id="deleteAddress" class="buttonAddress">
                    <label>Delete</label>
                </div>
            </div>
        </div>

    </div>
</div>
